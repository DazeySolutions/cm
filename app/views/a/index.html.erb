<% @events.each do |event| %>
<div class="col-xs-10 col-xs-offset-1 col-md-offset-0 col-md-6">
    <div class="well" style="padding: 0px;">
        <div class="start text-center">
            <p class="num"><%= event.start.strftime("%d") %></p>
            <p class="month"><%= event.start.strftime("%b") %></p>
        </div>
        <div>
           <img src="http://events.hbc-ky.com<%= event.image.url() %>" style='width:100%;'>
	</div>
        <div class="row">
            <div class="col-xs-12">
                <div class="col-xs-12">
                    <h4 class="col-xs-12"><%= event.title %></h4>
                    <p class="col-xs-12">
                        <%= event.description %>
                        <br />
                  	<i class="small">Start: <%= event.start.strftime('%b-%d-%y %I:%M %P') %> End: <%= event.end.strftime('%b-%d-%y %I:%M %P') %></i>
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
<% end %>
